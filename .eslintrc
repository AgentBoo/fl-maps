{
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
  },
  "plugins": [
    "react",
    "import",
    "babel"
  ],
  "rules": {
    'react/prop-types': 0,
    'babel/semi': 0,
    'no-return-assign': 0
  },
  "globals": {
    'google': 1
  }
}
